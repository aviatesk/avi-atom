"*":
  Hydrogen:
    autocompleteSuggestionPriority: 1
    gateways: "[{ \"name\": \"Local notebook\", \"options\": { \"baseUrl\": \"http://localhost:8888\", \"token\": \"JupyterLocalAuthToken\" } }, { \"name\": \"WSL notebook\", \"options\": { \"baseUrl\": \"http://localhost:8888\", \"token\": \"JupyterLocalAuthTokenWSL\"} }]"
    languageMappings: "{ \"julia\": \"weave.jl markdown\", \"common-lisp\": \"common lisp\", \"python\": \"pweave markdown\", \"r\": \"r markdown\" }"
    outputAreaDock: true
    showInspectorResultsInAutocomplete: true
    startDir: "dirOfFile"
    startupCode: "{\"Python 3\": \"%matplotlib inline\\nimport matplotlib.pyplot as plt\\nplt.style.use('dark_background')\", \"Python 3.6\": \"%matplotlib inline\\nimport matplotlib.pyplot as plt\\nplt.style.use('dark_background')\"}"
    wrapOutput: true
  "Zen-plus":
    fullscreen: false
    minimap: true
  "advanced-open-file":
    fuzzyMatch: true
    helmDirSwitch: true
  "atom-clock":
    dateFormat: "YYYY/MM/DD/HH:mm"
    showTooltip: true
  "atom-latex":
    build_after_save: false
    completion_suggestion_priority: 1
    delayed_minimap_refresh: true
    hide_log_if_success: true
    latexmk_param: "-synctex=1 -interaction=nonstopmode -file-line-error -shell-escape"
    preview_after_build: "Do nothing"
    toolchain: "latexmk toolchain"
  "atom-package-deps":
    ignored: [
      "hyperclick-latex"
    ]
  "atom-path-intellisense":
    "allowed-scopes": ".string.quoted"
  "autocomplete-paths":
    ignoredPatterns: []
    maxFileCount: 5000
    scopes: [
      {
        extensions: [
          "jpg"
          "jpeg"
          "png"
          "svg"
        ]
        prefixes: [
          "\\<img\\ssrc="
          "\\!\\[.*\\]\\("
        ]
        scopes: [
          "source.gfm"
          "source.weave.md"
          "source.pweave.md"
          "text.md"
        ]
      }
      {
        prefixes: [
          "\\[.*\\]\\("
          "\\[.*\\]\\:\\s"
        ]
        scopes: [
          "source.gfm"
          "source.weave.md"
          "source.pweave.md"
          "text.md"
        ]
      }
      {
        extensions: [
          ".tex"
          ".bib"
          ".tikz"
        ]
        prefixes: [
          "\\\\input{"
          "\\\\include(?:only)?{([^\\}]*?\\,\\s*)*"
          "\\\\(?:addbibresource|add(?:global|section)bib)(?:\\[.*?\\])?{"
          "^%\\s*!T[eE]X\\s+(root|bib)\\s*=\\s*"
        ]
        relative: true
        scopes: [
          "text.tex.latex"
          "text.tex.latex.tikz"
        ]
      }
      {
        extensions: [
          ".jpeg"
          ".jpg"
          ".png"
        ]
        prefixes: [
          "\\\\includegraphics(\\[.*?\\])?{"
        ]
        relative: true
        scopes: [
          "text.tex.latex"
          "text.tex.latex.tikz"
        ]
      }
      {
        extensions: [
          ".*"
        ]
        prefixes: [
          "\\\\inputminted(\\[.*?\\])?\\{.*\\}\\{"
        ]
        relative: true
        scopes: [
          "text.tex.latex"
          "text.tex.latex.tikz"
        ]
      }
    ]
  "autocomplete-plus":
    enableExtendedUnicodeSupport: true
    minimumWordLength: 2
  "bracket-matcher": {}
  core:
    allowPendingPaneItems: true
    audioBeep: false
    autoHideMenuBar: true
    automaticallyUpdate: false
    customFileTypes:
      "source.gfm": [
        "_draft.md"
        "CHANGELOG.md"
        "CODE_OF_CONDUCT.md"
        "CONTRIBUTING.md"
        "LICENSE.md"
        "README.md"
      ]
      "source.gfm.rmd": [
        "Rmd"
        "rmd"
      ]
      "source.java": [
        "jsh"
      ]
      "source.tsx": [
        "tsx"
      ]
      "text.md": [
        "md"
      ]
    disabledPackages: [
      "bookmarks"
      "deprecation-cop"
      "git-diff"
      "markdown-preview"
    ]
    excludeVcsIgnoredPaths: true
    ignoredNames: [
      "._*"
      ".hg"
      ".git"
      ".svn"
      ".DS_store"
      "Thumbs.db"
      "desktop.ini"
      "node_modules"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "Hydrogen"
      "Zen-plus"
      "atom-latex"
      "git-diff"
      "git-diff-plus"
      "git-plus"
      "git-time-machine"
      "hey-pane"
      "julia-client"
      "language-weave"
      "latex-tree"
      "markdown-preview-enhanced"
      "markdown-writer"
      "split-diff"
      "todo-show"
      "spell-check"
      "tree-view"
    ]
    packagesWithSnippetsDisabled: [
      "language-julia"
      "language-weave"
      "language-haskell"
    ]
    projectHome: "C:\\Users\\aviat\\github\\atom-pkgs"
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "ariake-dark-syntax"
    ]
    uriHandlerRegistration: "always"
    warnOnLargeFileLimit: 10
  "cursor-history":
    openInPendingState: true
    rowDeltaToRemember: 3
  "document-outline":
    showByDefault: false
  editor:
    fontFamily: "Fira Code, Source Han Code JP"
    fontSize: 15
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…．。．、，（）「」［］｛｝《》てにをはがのともへでや"
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
    softWrapHangingIndent: 1
    tabLength: 2
    zoomFontWhenCtrlScrolling: false
  "emacs-plus": {}
  "exception-reporting":
    userId: "9e026ba7-7268-13e9-0769-5cb4ad435f2f"
  "file-icons": {}
  "find-and-replace":
    closeFindPanelAfterSearch: true
    enablePCRE2: true
    focusEditorAfterSearch: true
    useRipgrep: true
  "fuzzy-finder": {}
  "git-diff-plus": {}
  "git-plus":
    diffs:
      useSplitDiff: true
      wordDiff: true
    experimental:
      autoFetch: 0
      customCommands: true
      diffBranches: true
    general:
      enableStatusBarIcon: false
      showBranchInTreeView: false
    remoteInteractions:
      promptForBranch: true
  "hey-pane":
    expandedWidth: 95
    followLocations:
      bottom: false
      left: false
      right: false
    followModeByDefault: false
  "highlight-line":
    enableBackgroundColor: false
    hideHighlightOnSelect: true
  "highlight-selected":
    onlyHighlightWholeWords: false
  ink:
    terminal:
      ansiBlue:
        dark: "#f5faff"
      ansiBrightBlue:
        dark: "#f5faff"
      ansiBrightCyan:
        dark: "#93ddfb"
      ansiBrightGreen:
        dark: "#a571f4"
      ansiBrightMagenta:
        dark: "#dda2f6"
      ansiBrightRed:
        dark: "#4d8acb"
      ansiBrightYellow:
        dark: "#86b1e0"
      ansiCyan:
        dark: "#93ddfb"
      ansiGreen:
        dark: "#a571f4"
      ansiMagenta:
        dark: "#dda2f6"
      ansiRed:
        dark: "#4d8acb"
      ansiYellow:
        dark: "#86b1e0"
