  "split-diff":
    ignoreWhitespace: true
    muteNotifications: true
    syncHorizontalScroll: true
    turnOffSoftWrap: true
  "status-bar":
    cursorPositionFormat: "%L L | %C C"
    selectionCountFormat: "(%L L | %C Char)"
  "todo-show":
    autoRefresh: false
    exportAs: "Table"
    findTheseTodos: [
