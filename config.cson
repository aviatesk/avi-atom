      whitelistedKeybindingsTerminal: [
        "ctrl-tab"
        "ctrl-shift-tab"
        "ctrl-alt-right"
        "ctrl-alt-left"
        "ctrl-w"
        "ctrl-v"
        "alt-:"
        "ctrl-t"
        "alt-t"
        "alt-b"
        "alt-l"
        "alt-h"
        "alt-shift-h"
        "ctrl-h"
        "alt-x"
      ]
    firstBoot: false
    juliaOptions:
      arguments: []
      autoCompletionSuggestionPriority: 1
      bootMode: "Cycler"
      noAutoParenthesis: true
      optimisationLevel: 2
      persistWorkingDir: false
      startupArguments: []
      workingDir: ""
    juliaPath: "julia"
    uiOptions:
      cellDelimiter: [
        "#%%"
        "# %%"
      ]
      enableMenu: true
      errorInRepl: true
      layouts:
        debuggerPane:
          split: "up"
        defaultPanes:
          outline: true
          plotPane: false
        documentation:
          defaultLocation: "right"
          split: "up"
        linter:
          defaultLocation: "right"
          split: "down"
        openDefaultPanesOnStartUp: false
        outline:
          defaultLocation: "right"
          split: "up"
        plotPane:
          defaultLocation: "right"
          split: "down"
        workspace:
          defaultLocation: "right"
          split: "down"
  "language-markdown":
