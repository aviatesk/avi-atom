        "ctrl-shift-tab"
        "ctrl-alt-right"
        "ctrl-alt-left"
        "ctrl-w"
        "ctrl-v"
        "alt-:"
        "ctrl-t"
        "alt-t"
        "alt-b"
        "alt-l"
        "alt-h"
        "alt-shift-h"
        "ctrl-h"
        "alt-x"
        "ctrl-c"
        "ctrl-l"
        "alt-i"
        "alt-p"
        "alt-w"
        "alt-j"
        "alt-,"
        "alt-f"
        "alt-m"
        "alt-d"
        "f8"
        "shift-f8"
        "f9"
        "f10"
        "ctrl-f10"
        "shift-f10"
